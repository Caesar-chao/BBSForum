<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.yujian.mapper.AdminMapper">


    <select id="queryAdminByUsernameAndPassword" resultType="com.yujian.pojo.Admin">
        select * from bbs.admin
        where username = #{username} and password = #{password};
    </select>


</mapper>

